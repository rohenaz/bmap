{
	"hosting": {
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "/tx/*",
				"function": "decode"
			},
			{
				"source": "/tx/*/*",
				"function": "decode"
			}
		]
	},
	"functions": {
		"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"]
	}
}
