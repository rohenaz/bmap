{
  "name": "bmapjs",
  "version": "0.4.0-beta.7",
  "description": "A transaction parser for Bitcoin data protocols like B, MAP, BAP, METANET and AIP/HAIP.",
  "types": "types/index.d.ts",
  "legacy": "dist/bmap.cjs",
  "main": "dist/bmap.js",
  "type": "module",
  "targets": {
    "main": {
      "isLibrary": true,
      "context": "node",
      "source": "src/bmap.ts",
      "engines": {
        "node": ">=12"
      }
    },
    "types": {
      "source": "src/bmap.ts"
    },
    "legacy": {
      "includeNodeModules": true,
      "distDir": "dist",
      "source": "src/browser.ts",
      "optimize": true,
      "scopeHoist": true,
      "context": "browser",
      "outputFormat": "commonjs",
      "engines": {
        "browsers": "Chrome 80"
      }
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "serve": "npm run build && node ./node_modules/ws/index.js",
    "clean": "yarn cache clean && rimraf dist/ types/index.d.ts parcel-bundle-reports .parcel-cache",
    "build": "rimraf parcel-bundle-reports dist/ types/index.d.ts && parcel build . --target types --target main --target legacy --target module --reporter @parcel/reporter-bundle-analyzer",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "set NODE_OPTIONS=--openssl-legacy-provider&&jest --collectCoverage",
    "testwatch": "./node_modules/.bin/jest --watchAll --collectCoverage",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testwatch",
    "check": "./node_modules/.bin/npm-check -u",
    "pub": "./node_modules/.bin/np || true",
    "build-public": "set NODE_ENV=production&&npm run build && ncp dist/bmap.js public/bmap.js && ncp dist/bmap.js.map public/bmap.js.map && ncp dist/bmap.cjs public/bmap.cjs && ncp dist/bmap.cjs.map public/bmap.cjs.map",
    "build-all": "npm run lint && npm run build-public",
    "watch": "parcel-watch",
    "public": "yarn build-public && cd public && ws --http2",
    "publish": "npm run build-public && npm publish",
    "publish-beta": "git tag beta && npm run build-public && npm publish --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohenaz/bmap.git"
  },
  "keywords": [
    "bitcoin",
    "bmap",
    "magic",
    "attribute",
    "protocol",
    "bsv"
  ],
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@jest/types": "^29.3.1",
    "@parcel/compressor-brotli": "^2.8.2",
    "@parcel/compressor-gzip": "^2.8.2",
    "@parcel/reporter-bundle-analyzer": "^2.8.2",
    "@parcel/transformer-typescript-tsc": "^2.8.2",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "bpu": "^0.0.13",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-cli": "29.3.1",
    "jest-config": "29.3.1",
    "ncp": "^2.0.0",
    "np": "2.20.1",
    "parcel": "^2.8.2",
    "prettier": "^2.8.1",
    "rimraf": "2.6.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yarn": "^1.22.19"
  },
  "author": "Satchmo",
  "license": "Open BSV",
  "bugs": {
    "url": "https://github.com/rohenaz/bmap/issues"
  },
  "alias": {
    "bsv": "@ts-bitcoin/core"
  },
  "homepage": "https://github.com/rohenaz/bmap#readme",
  "dependencies": {
    "@moneybutton/paymail-client": "^0.39.0",
    "@msgpack/msgpack": "^2.8.0",
    "@ts-bitcoin/core": "^2.2.0",
    "boostpow": "^1.8.1",
    "buffer": "^5.5.0",
    "node-fetch": "^2.6.1",
    "stream-browserify": "^3.0.0"
  }
}